  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env -e .env.dev -- nest start --watch",
    "start:debug": "dotenv -e .env -e .env.dev -- nest start --debug --watch",
    "start:prod": "dotenv -e .env -e .env.prod -- node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:user": "jest --testPathPattern=src/user",
    "test:auth": "jest --testPathPattern=src/auth",
    "test:template": "jest --testPathPattern=src/template",
    "test:notification": "jest --testPathPattern=src/notification",
    "test:cov:user": "jest --coverage --testPathPattern=src/user",
    "test:cov:auth": "jest --coverage --testPathPattern=src/auth",
    "test:ci": "jest --ci --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm typeorm migration:generate db/migrations/Migration",
    "migration:run": "pnpm typeorm migration:run",
    "migration:revert": "pnpm typeorm migration:revert",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js"
  },
